<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="aubo_i5" params="prefix
    *shoulder_calibration
    *upperArm_calibration
    *foreArm_calibration
    *wrist1_calibration
    *wrist2_calibration
    *wrist3_calibration
    *wrist3_limit">

    <link name="${prefix}base_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/visual/base_link.DAE" />
        </geometry>
        <material name="">
          <color rgba="1 1 1 1" />
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/collision/base_link.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="-1.4795E-13 0.0015384 0.020951" rpy="0 0 0" />
        <mass value="0.83419" />
        <inertia
          ixx="0.0014414" ixy="7.8809E-15" ixz="8.5328E-16"
          iyy="0.0013542" iyz="-1.4364E-05"
          izz="0.0024659" />
      </inertial>
    </link>

    <joint name="${prefix}shoulder_joint" type="revolute">
      <origin xyz="0 0 0.122" rpy="0 0 ${pi}" />
      <parent link="${prefix}base_link" />
      <child link="${prefix}shoulder_Link" />
      <axis xyz="0 0 1" />
      <xacro:insert_block name="shoulder_calibration"/>
      <limit lower="-3.05" upper="3.05" velocity="2.62" effort="207.0"/>
    </joint>

    <link name="${prefix}shoulder_Link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/visual/shoulder_Link.DAE" />
        </geometry>
        <material name="">
          <color rgba="1 1 1 1" />
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/collision/shoulder_Link.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0.0 0.006 -0.002" rpy="0 0 0" />
        <mass value="5.05" />
        <inertia
          ixx="0.0148" ixy="0" ixz="0"
          iyy="0.0143" iyz="0"
          izz="0.0095" />
      </inertial>
    </link>

    <joint name="${prefix}upperArm_joint" type="revolute">
      <origin xyz="0 0.1215 0" rpy="${-pi/2} ${-pi/2} 0" />
      <parent link="${prefix}shoulder_Link" />
      <child link="${prefix}upperArm_Link" />
      <axis xyz="0 0 1" />
      <xacro:insert_block name="upperArm_calibration"/>
      <limit lower="-3.05" upper="3.05" velocity="2.62" effort="207.0"/>
    </joint>

    <link name="${prefix}upperArm_Link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/visual/upperArm_Link.DAE" />
        </geometry>
        <material name="">
          <color rgba="1 1 1 1" />
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/collision/upperArm_Link.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0.304 0.0 -0.03" rpy="0 0 0" />
        <mass value="11.90" />
        <inertia
          ixx="0.0305" ixy="0" ixz="0"
          iyy="0.4505" iyz="0"
          izz="0.0095" />
      </inertial>
    </link>

    <joint name="${prefix}foreArm_joint" type="revolute">
      <origin xyz="0.408 0 0" rpy="${-pi} 0 0" />
      <parent link="${prefix}upperArm_Link" />
      <child link="${prefix}foreArm_Link" />
      <axis xyz="0 0 1" />
      <xacro:insert_block name="foreArm_calibration"/>
      <limit lower="-3.05" upper="3.05" velocity="2.62" effort="207.0"/>
    </joint>

    <link name="${prefix}foreArm_Link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/visual/foreArm_Link.DAE" />
        </geometry>
        <material name="">
          <color rgba="1 1 1 1" />
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/collision/foreArm_Link.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0.415 0.0 0.0992" rpy="0 0 0" />
        <mass value="3.3" />
        <inertia
          ixx="0.0033" ixy="0" ixz="0.0009"
          iyy="0.0575" iyz="0"
          izz="0.0565" />
      </inertial>
    </link>

    <joint name="${prefix}wrist1_joint" type="revolute">
      <origin xyz="0.376 0 0" rpy="${pi} 0 ${pi/2}" />
      <parent link="${prefix}foreArm_Link" />
      <child link="${prefix}wrist1_Link" />
      <axis xyz="0 0 1" />
      <xacro:insert_block name="wrist1_calibration"/>
      <limit lower="-3.05" upper="3.05" velocity="3.14" effort="34.0"/>
    </joint>

    <link name="${prefix}wrist1_Link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/visual/wrist1_Link.DAE" />
        </geometry>
        <material name="">
          <color rgba="1 1 1 1" />
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/collision/wrist1_Link.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0. 0.028 -0.003" rpy="0 0 0" />
        <mass value="1.62" />
        <inertia
          ixx="0.0023" ixy="0" ixz="0"
          iyy="0.0013" iyz="0"
          izz="0.0022" />
      </inertial>
    </link>

    <joint name="${prefix}wrist2_joint" type="revolute">
      <origin xyz="0 0.1025 0" rpy="${-pi/2} 0 0" />
      <parent link="${prefix}wrist1_Link" />
      <child link="${prefix}wrist2_Link" />
      <axis xyz="0 0 1" />
      <xacro:insert_block name="wrist2_calibration"/>
      <limit lower="-3.05" upper="3.05" velocity="3.14" effort="34.0"/>
    </joint>

    <link name="${prefix}wrist2_Link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/visual/wrist2_Link.DAE" />
        </geometry>
        <material name="">
          <color rgba="1 1 1 1" />
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/collision/wrist2_Link.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0. 0.0028 -0.003" rpy="0 0 0" />
        <mass value="1.62" />
        <inertia
          ixx="0.0023" ixy="0" ixz="0"
          iyy="0.0013" iyz="0"
          izz="0.0022" />
      </inertial>
    </link>

    <joint name="${prefix}wrist3_joint" type="revolute">
      <origin xyz="0 -0.094 0" rpy="${pi/2} 0 0" />
      <parent link="${prefix}wrist2_Link" />
      <child link="${prefix}wrist3_Link" />
      <axis xyz="0 0 1" />
      <xacro:insert_block name="wrist3_calibration"/>
      <xacro:insert_block name="wrist3_limit"/>
    </joint>

    <link name="${prefix}wrist3_Link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/visual/wrist3_Link.DAE" />
        </geometry>
        <material name="">
          <color rgba="1 1 1 1" />
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://aubo_description/meshes/aubo_i5/collision/wrist3_Link.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0.0 0.0 -0.0071" rpy="0 0 0" />
        <mass value="0.15" />
        <inertia
          ixx="7.31376E-5" ixy="0" ixz="0"
          iyy="7.19528E-5" iyz="0"
          izz="0.00018" />
      </inertial>
    </link>

  </xacro:macro>

</robot>
